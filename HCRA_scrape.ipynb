    "#initialize each builder profile\n",
    "\n",
    "class HCRA_builder:\n",
    "    link = ''\n",
    "    location = ''\n",
    "    licensed = ''\n",
    "    license_status = ''\n",
    "    legal_name = ''\n",
    "    business_name = ''\n",
    "    license_start = ''\n",
    "    license_expiry = ''\n",
    "    total_possessions = ''\n",
    "    total_freehold = ''\n",
    "    total_condo  = ''\n",
    "    notice_issued = ''\n",
    "    disciplinary_activity = ''\n",
    "    court_charges = ''\n",
    "    chargeable_conciliations = ''\n",
    "    paid_in_claim = ''\n",
    "    outstanding_claims = ''\n",
    "    breach = ''\n",
    "    tarion_action = ''\n",
    "    address = ''\n",
    "    email = ''\n",
    "    fax = ''\n",
    "    phone = ''\n",
    "    website = ''\n",
    "    \n",
    "    def __init__(self,link):\n",
    "        self.link = link\n",
    "    \n",
    "    def to_dict(self):     \n",
    "        return {\n",
    "            'location': self.location,\n",
    "            'licensed': self.licensed,\n",
    "            'license status': self.license_status,\n",
    "            'legal name': self.legal_name,\n",
    "            'business name': self.business_name,\n",
    "            'Licence Expiry Date': self.license_expiry,\n",
    "            'Initially Licensed': self.license_start,\n",
    "            'total possessions': self.total_possessions,\n",
    "            'total freehold properties': self.total_freehold,\n",
    "            'condominium units': self.total_condo,\n",
    "            'notice and/or orders issued': self.notice_issued,\n",
    "            'disciplinary activities': self.disciplinary_activity,\n",
    "            'court charges and/or convictions': self.court_charges,\n",
    "            'chargeable conciliations': self.chargeable_conciliations,\n",
    "            'total dollars paid in claims': self.paid_in_claim,\n",
    "            'outstanding claims paid owing': self.outstanding_claims,\n",
    "            'breach': self.breach,\n",
    "            'Tarion action on behalf of vendor/builder': self.tarion_action, \n",
    "            'link': self.link,\n",
    "            'Address': self.address,\n",
    "            'Email': self.email,\n",
    "            'Fax Number': self.fax,\n",
    "            'Phone Number': self.phone,\n",
    "            'Website': self.website\n",
    "        }  \n",
    "    \n",
    "    \n",
    "    \n",
    "#get properties infomation for builders through time form \"properties\" and \"condo projects\" tabs\n",
    "\n",
    "class HCRA_builder_year:\n",
    "    link = ''\n",
    "    year = ''\n",
    "    current_possessions = ''\n",
    "    current_freehold = ''\n",
    "    current_condo  = ''\n",
    "    current_chargeable_conciliations = ''\n",
    "    SW_num_homes_claims = ''\n",
    "    SW_pct_homes_claims = ''\n",
    "    SW_current_paid_claim = ''\n",
    "    other_num_homes_claims = ''\n",
    "    other_current_paid_claim = ''\n",
    "    all_current_paid_claim = ''\n",
    "\n",
    "    def __init__(self,link,year):\n",
    "        self.link = link\n",
    "        self.year = year\n",
    "    \n",
    "    def to_dict(self):     \n",
    "        return {\n",
    "            'link': self.link,\n",
    "            'year': self.year,\n",
    "            'current possessions': self.current_possessions,\n",
    "            'current freehold properties': self.current_freehold,\n",
    "            'current condominium units': self.current_condo,\n",
    "            'current chargeable conciliations': self.current_chargeable_conciliations,\n",
    "            'number of homes structural warranty claims': self.SW_num_homes_claims,\n",
    "            'percent of homes structural warranty claims': self.SW_pct_homes_claims,\n",
    "            'dollar paid structural warranty claims': self.SW_current_paid_claim,\n",
    "            'number of homes other claims': self.other_num_homes_claims,\n",
    "            'dollar paid other claims': self.other_current_paid_claim,\n",
    "            'dollar paid all claims': self.all_current_paid_claim\n",
    "        }\n",
    "    \n",
    "class HCRA_related_company:\n",
    "    link = ''\n",
    "    related_link = ''\n",
    "    \n",
    "    def __init__(self,link):\n",
    "        self.link = link\n",
    "    \n",
    "    def to_dict(self):     \n",
    "        return {\n",
    "            'link': self.link,\n",
    "            'related link': self.related_link,\n",
    "        }     \n",
    "\n",
    "class HCRA_condo_project:\n",
    "    link = ''\n",
    "    name = ''\n",
    "    status = ''\n",
    "    location  = ''\n",
    "    address = ''\n",
    "    units = ''\n",
    "    vendor_terminated = ''\n",
    "    last_update = ''\n",
    "    \n",
    "    def __init__(self,link):\n",
    "        self.link = link\n",
    "    \n",
    "    def to_dict(self):     \n",
    "        return {\n",
    "            'link': self.link,\n",
    "            'project name': self.name,\n",
    "            'status': self.status,\n",
    "            'location': self.location,\n",
    "            'address': self.address,\n",
    "            'number of units': self.units,\n",
    "            'prchs terminated vendor': self.vendor_terminated,\n",
    "            'last_update': self.last_update\n",
    "        } "
   ]

    "#get information on all profiles\n",
    "\n",
    "##initialize \n",
    "profile_link_list = pd.read_csv(\"profile_list.csv\")\n",
    "builders_dict = []\n",
    "missed_profile_link_list = []\n",
    "builder_year_dict = []\n",
    "projects_dict = []\n",
    "related_companies_dict = []\n",
    "\n",
    "def chunks(xs, n):  #breaking a list into chunks of equal size\n",
    "    n = max(1, n)\n",
    "    return (xs[i:i+n] for i in range(0, len(xs), n))\n",
    "\n",
    "browser = webdriver.Chrome()\n",
    "\n",
    "valid_profile_list = profile_link_list[profile_link_list['license status'].isin(['Licensed','Expired'])]  #choose builders that had license at some point\n",
    "\n",
    "##loop over all builder profiles and get information\n",
    "for index, profile in valid_profile_list.iterrows():   \n",
    "\n",
    "    try:\n",
    "        builder = HCRA_builder(profile['link'])\n",
    "        builder.link = profile['link']\n",
    "        builder.location = profile['location']\n",
    "        builder.licensed = profile['licensed']\n",
    "        builder.license_status = profile['license status']   \n",
    "\n",
    "        browser.get(builder.link)\n",
    "        WebDriverWait(browser, 100).until(EC.presence_of_element_located((By.XPATH,\"//h1\")))  #wait to load\n",
    "        \n",
    "        if browser.find_elements(By.XPATH,\"//p\")[0].text!='Sorry, no builder matching that ID can be found.': #some links are broken. this line checks for that\n",
    "    ###get information from overview:\n",
    "            builder.legal_name = browser.find_element(By.XPATH,\"//h1\").text\n",
    "            builder.business_name = browser.find_element(By.XPATH,\"//h2\").text\n",
    "\n",
    "            license_info = [i.text for i in browser.find_elements(By.XPATH,\"//time\")][:2]\n",
    "            builder.license_expiry = license_info[0]\n",
    "            builder.license_start = license_info[1]\n",
    "\n",
    "            info = [item.text for item in browser.find_elements(By.CLASS_NAME,\"number\")]\n",
    "            builder.total_possessions = info[0]\n",
    "            builder.total_freehold = info[1]\n",
    "            builder.total_condo = info[2]\n",
    "            builder.notice_issued = info[3]\n",
    "            builder.disciplinary_activity = info[4]\n",
    "            builder.court_charges = info[5]\n",
    "            builder.chargeable_conciliations = info[6]\n",
    "            builder.paid_in_claim = info[7]\n",
    "            builder.outstanding_claims = info[8]\n",
    "            builder.breach = info[9]\n",
    "            builder.tarion_action = info[10]\n",
    "\n",
    "            contact_info = list(filter(None, [i.text for i in browser.find_elements(By.XPATH,\"//p\")] ))\n",
    "            contact_info_colnames = [i.text for i in browser.find_elements(By.XPATH,\"//p/preceding-sibling::*\")] \n",
    "            if len(contact_info_colnames)<5:\n",
    "                missing_cols=set(['Address','Email','Fax Number','Phone Number','Website']\n",
    "                                          ).difference(contact_info_colnames)\n",
    "                contact_info_colnames += missing_cols\n",
    "                contact_info += [\"\"] * len(missing_cols)\n",
    "            contact_info = [x for y, x in sorted(zip(contact_info_colnames, contact_info))]\n",
    "        #contact info needs to be sorted to get the same order for every page. some of them miss one of the items\n",
    "            builder.address = contact_info[0]\n",
    "            builder.email = contact_info[1]\n",
    "            builder.fax = contact_info[2]\n",
    "            builder.phone = contact_info[3]\n",
    "            builder.website = contact_info[4]\n",
    "\n",
    "\n",
    "            builders_dict.append(builder.to_dict()) #add builder information to the data\n",
    "\n",
    "\n",
    "\n",
    "        ###get to \"properties\" page:\n",
    "            element = WebDriverWait(browser, 100).until(EC.element_to_be_clickable((By.ID, \"properties\")))\n",
    "            element.click()\n",
    "            WebDriverWait(browser, 100).until(EC.any_of(\n",
    "                    EC.visibility_of_element_located((By.XPATH,\"//section/div/table/tbody/tr\")),\n",
    "                    EC.visibility_of_element_located((By.XPATH, \"//div[contains(@class, 'Alert_alert__2d52x Alert_flex__Mh3UF')]\"))\n",
    "                )\n",
    "            ) \n",
    "        #get information on the top table\n",
    "            if browser.find_elements(By.XPATH,\"//section/div/table/tbody/tr\"):\n",
    "                table_rows = browser.find_elements(By.XPATH,\"//section/div/table/tbody/tr\")\n",
    "                table_rows.pop()  #last row is just totals\n",
    "\n",
    "                for row in table_rows:\n",
    "                    row_vals = row.text.split() #[x.strip('$%,') for x in row.text.split()] \n",
    "                    builder_year = HCRA_builder_year(builder.link,row_vals[0])\n",
    "\n",
    "                    [builder_year.year,\n",
    "                     builder_year.current_freehold,\n",
    "                     builder_year.current_condo,\n",
    "                     builder_year.current_possessions, \n",
    "                     builder_year.current_chargeable_conciliations,\n",
    "                     builder_year.SW_num_homes_claims,\n",
    "                     builder_year.SW_pct_homes_claims,\n",
    "                     builder_year.SW_current_paid_claim,\n",
    "                     builder_year.other_num_homes_claims,\n",
    "                     builder_year.other_current_paid_claim,\n",
    "                     builder_year.all_current_paid_claim] = row_vals\n",
    "                    builder_year_dict.append(builder_year.to_dict()) #add builder information to the data\n",
    "\n",
    "\n",
    "\n",
    "        ###get to \"condo projects\" page:\n",
    "            element = WebDriverWait(browser, 1000).until(EC.element_to_be_clickable((By.ID, \"condoProjects\")))\n",
    "            element.click()\n",
    "            WebDriverWait(browser, 1000).until(EC.any_of(\n",
    "                    EC.visibility_of_element_located((By.XPATH,\"//table/tbody/tr\")),\n",
    "                    EC.visibility_of_element_located((By.XPATH, \"//div[contains(@class, 'Alert_alert__2d52x Alert_flex__Mh3UF')]\"))\n",
    "                )\n",
    "            ) \n",
    "        #get information on the projects table\n",
    "            if browser.find_elements(By.XPATH,\"//tbody/tr/td\"):\n",
    "                page = 1\n",
    "                while 1:\n",
    "                # any page\n",
    "                    table_entries = browser.find_elements(By.XPATH,\"//tbody/tr/td\")\n",
    "                    entries=[]\n",
    "                    for el in table_entries:\n",
    "                        if (not any(word in el.text for word in ['Vendor','Zoning','Insolvency','Receivership','not met','not obtained','Other']) ): \n",
    "                        #format of the table chenges when there is a cancelled project. I just drop the cancelation reason which makes entries like other projects\n",
    "                            entries.append(el.text)\n",
    "                    rows = list(chunks(entries,7))\n",
    "                    for row in rows:\n",
    "                        project = HCRA_condo_project(builder.link)\n",
    "                        [project.name,\n",
    "                        project.status,\n",
    "                        project.location,\n",
    "                        project.address,\n",
    "                        project.units,\n",
    "                        project.vendor_terminated,\n",
    "                        project.last_update] = row\n",
    "                        projects_dict.append(project.to_dict()) #add condo project information to the data        \n",
    "\n",
    "                    if browser.find_elements(By.CLASS_NAME,\"PaginationNav_paginationNav__9Mbc1\")==[]: #if there is only one page to the table\n",
    "                        break\n",
    "                    if  page>1 and browser.find_elements(By.CLASS_NAME,\"PaginationNav_disabled__26kFc\")!=[]: #if in the last table page\n",
    "                        break\n",
    "                    else:\n",
    "                        page+=1\n",
    "                        browser.find_elements(By.XPATH,\"//nav/ul/li/button\")[-2].click()  #go next page                \n",
    "\n",
    "\n",
    "\n",
    "        ###get related companies if existed (related companies share the same management or personel)\n",
    "            if browser.find_elements(By.ID,\"members\")[0].get_attribute('disabled')!='true':\n",
    "                element = WebDriverWait(browser, 1).until(EC.element_to_be_clickable((By.ID, \"members\")))\n",
    "                element.click()\n",
    "                WebDriverWait(browser, 100).until(EC.presence_of_element_located((By.XPATH,\"//tbody\")))\n",
    "                page = 1\n",
    "                while 1:\n",
    "                    companies = [x.get_attribute(\"href\") for x in browser.find_elements(By.XPATH,\"//tbody/tr/td/a\")]\n",
    "                    for item in companies:\n",
    "                        company = HCRA_related_company(builder.link)\n",
    "                        company.related_link = item\n",
    "\n",
    "                        related_companies_dict.append(company.to_dict()) #add related companies information to the data\n",
    "\n",
    "                    if browser.find_elements(By.CLASS_NAME,\"PaginationNav_paginationNav__9Mbc1\")==[]: #if there is only one page to the table\n",
    "                        break\n",
    "                    if  page>1 and browser.find_elements(By.CLASS_NAME,\"PaginationNav_disabled__26kFc\")!=[]: #if in the last table page\n",
    "                        break\n",
    "                    else:\n",
    "                        page+=1\n",
    "                        browser.find_elements(By.XPATH,\"//nav/ul/li/button\")[-2].click()  #go next page                \n",
    "                    \n",
    "    except:\n",
    "        missed_profile_link_list.append(profile['link'])\n",
    "        print(profile['link'])\n",
    "        pass\n",
    "\n",
    "\n",
    "## save everything\n",
    "builder_profiles_df = pd.DataFrame(builders_dict)\n",
    "builder_profiles_df.to_csv(\"all_licensed_builders.csv\" , index=False)\n",
    "\n",
    "builder_year_profiles_df = pd.DataFrame(builder_year_dict)\n",
    "builder_year_profiles_df.to_csv(\"builder_time.csv\" , index=False)\n",
    "\n",
    "condo_projects_df = pd.DataFrame(projects_dict)\n",
    "condo_projects_df.to_csv(\"condo_projects.csv\" , index=False)\n",
    "\n",
    "related_companies_df = pd.DataFrame(related_companies_dict)\n",
    "related_companies_df.to_csv(\"related_companies.csv\" , index=False)\n",
    "\n",
    "##save problematic links for later\n",
    "with open('missed_profile_link_list.txt', 'w') as f:\n",
    "    for link in missed_profile_link_list:\n",
    "        f.write(link+\"\\n\")"
   ]
  }

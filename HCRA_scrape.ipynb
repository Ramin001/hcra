    "##get link to all profiles in website\n",
    "profile_link_list = pd.DataFrame(columns=['link','legal name','business name','location','licensed','license status'])\n",
    "page = 1\n",
    "\n",
    "browser = webdriver.Chrome()\n",
    "browser.get('https://obd.hcraontario.ca/buildersearchresults?&page=1') #search page containing all builders \n",
    "WebDriverWait(browser, 50).until(EC.presence_of_element_located((By.XPATH,\"//tbody\")))  #wait to load\n",
    "\n",
    "while 1:\n",
    "    table_rows = browser.find_elements(By.XPATH,\"//tbody/tr\")\n",
    "    for i in range(0,len(table_rows)):\n",
    "        link = [x.get_attribute(\"href\") for x in table_rows[i].find_elements(By.XPATH,\"*/*\")][0]\n",
    "        info = [x.text for x in table_rows[i].find_elements(By.XPATH,\"*\")]\n",
    "        if len(info)==5:\n",
    "            profile_link_list.loc[len(profile_link_list)] = [link] + info\n",
    "        elif len(info)==4:  #happens in case of parent companies: 'This is an umbrella company and therefore does not have a distinct registration number.'\n",
    "            profile_link_list.loc[len(profile_link_list)] = [link] + info[:3] + [\"umbrella company\"]*2\n",
    "        \n",
    "    if  page>1 and browser.find_elements(By.CLASS_NAME,\"PaginationNav_disabled__26kFc\")!=[]: #if in the last search page\n",
    "        break\n",
    "    else:\n",
    "        page+=1\n",
    "        browser.find_elements(By.XPATH,\"//nav/ul/li/button\")[-2].click()  #go next page\n",
    "\n",
    "        \n",
    "profile_link_list.to_csv(\"profile_list.csv\",  index=False)"
   ]
